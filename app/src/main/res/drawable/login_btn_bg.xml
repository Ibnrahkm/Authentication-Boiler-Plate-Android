<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <size android:height="@dimen/_50sdp" android:width="@dimen/_150sdp"></size>
            <stroke android:color="#7C8BB8" android:width="@dimen/_1sdp" android:dashWidth="@dimen/_1sdp"
                  >
            </stroke>
            <corners android:radius="@dimen/_5sdp"></corners>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <size android:width="@dimen/_150sdp" android:height="@dimen/_50sdp"></size>
            <stroke android:color="#ffffff" android:width="@dimen/_1sdp" android:dashWidth="@dimen/_1sdp"
                   >
            </stroke>
            <corners android:radius="@dimen/_5sdp"></corners>

        </shape>
    </item>

</selector>